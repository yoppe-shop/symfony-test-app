<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ShopBundle\Entity\ProductsOptionsValues" table="products_options_values">
    <id name="productsOptionsValuesId" type="integer" column="products_options_values_id"/>
    <id name="languageId" type="integer" column="language_id"/>
    <field name="productsOptionsValuesName" type="string" column="products_options_values_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
